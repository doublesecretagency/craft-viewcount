{
    "name": "doublesecretagency/craft-viewcount",
    "description": "Count the number of times an element has been viewed.",
    "type": "craft-plugin",
    "version": "1.1.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "view-count",
        "views",
        "counter"
    ],
    "support": {
        "docs": "https://www.doublesecretagency.com/plugins/view-count/docs",
        "issues": "https://github.com/doublesecretagency/craft-viewcount/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Double Secret Agency",
            "homepage": "https://www.doublesecretagency.com/plugins"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
          "doublesecretagency\\viewcount\\": "src/"
        }
    },
    "extra": {
        "name": "View Count",
        "handle": "view-count",
        "class": "doublesecretagency\\viewcount\\ViewCount"
    }
}
